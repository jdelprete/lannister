<div class="panel margin-bottom order" data-order-id="<%= order.id %>">
  <div>
    <div class="panel-header">
      <div class="container-flex flex-between">
        <div class="container-flex">
          <div class="order-flags space-small-right svg-container">
            <span><svg class="icon icon-bookmark-o">
            <use xlink:href="/img/app-icons.svg#icon-bookmark-o" xmlns:xlink="http://www.w3.org/1999/xlink"></use></svg></span>
            <ul>
              <li class="active">
                <a href="#"><svg class="icon icon-bookmark-o">
                <use xlink:href="/img/app-icons.svg#icon-bookmark-o" xmlns:xlink="http://www.w3.org/1999/xlink"></use></svg></a>
              </li>
              <li class="">
                <a href="#"><svg class="icon icon-bookmark color-info">
                <use xlink:href="/img/app-icons.svg#icon-bookmark" xmlns:xlink="http://www.w3.org/1999/xlink"></use></svg></a>
              </li>
              <li class="">
                <a href="#"><svg class="icon icon-bookmark color-primary">
                <use xlink:href="/img/app-icons.svg#icon-bookmark" xmlns:xlink="http://www.w3.org/1999/xlink"></use></svg></a>
              </li>
              <li class="">
                <a href="#"><svg class="icon icon-bookmark color-success">
                <use xlink:href="/img/app-icons.svg#icon-bookmark" xmlns:xlink="http://www.w3.org/1999/xlink"></use></svg></a>
              </li>
              <li class="">
                <a href="#"><svg class="icon icon-bookmark color-warning">
                <use xlink:href="/img/app-icons.svg#icon-bookmark" xmlns:xlink="http://www.w3.org/1999/xlink"></use></svg></a>
              </li>
              <li class="">
                <a href="#"><svg class="icon icon-bookmark color-danger">
                <use xlink:href="/img/app-icons.svg#icon-bookmark" xmlns:xlink="http://www.w3.org/1999/xlink"></use></svg></a>
              </li>
            </ul>
          </div>
          <h3 class="nowrap space-small-top display-block"><span class="elephant-color">Order</span> <a href="https://storename.myshopify.com/admin/orders/<%= order.shopify_id %>" target="_blank">#<%= order.shopify_order_number %></a> <span class="elephant-color"><%= order.ordered_at.strftime('%Y-%m-%d') %></span></h3>
          <div class="order-status space-small-left svg-container">
            <svg class="float-left icon-unfulfilled color-warning" data-original-title="" title="">
            <use xlink:href="/img/app-icons.svg#icon-unfulfilled" xmlns:xlink="http://www.w3.org/1999/xlink"></use></svg> <svg class="float-left icon-financial color-success" data-original-title="" title="">
            <use xlink:href="/img/app-icons.svg#icon-financial" xmlns:xlink="http://www.w3.org/1999/xlink"></use></svg>
          </div>
          <div class="margin-left">
            <span class="grey-color">Customer:</span> <a href="#"><%= order.shipping_address['name'] %></a>
          </div>
          <div class="margin-small-left flag-icon flag-size flag-icon-<%= order.shipping_address['country_code'].downcase %>"></div>
        </div>
        <div class="container-flex align-right">
          <!---->
           <button class="btn btn-default margin-left" type="button">Mark as Shipped</button>
        </div>
      </div>
    </div>
    <div class="panel-header container-flex flex-top" style="display: none;">
      <button class="close btn-sm btn-icon-only btn-transparent"><span class="btn-icon-wrap"><svg>
      <use xlink:href="/images/icons.svg#icon-close" xmlns:xlink="http://www.w3.org/1999/xlink"></use></svg></span></button>
      <div class="margin-right flex-item-grid mb10">
        <div class="border-bottom mb10">
          <label><strong class="grey-color">Name:</strong></label>
          <div class="mb10 clearfix">
            <div class="float-left">
              <%= order.shipping_address['name'] %>
            </div>
            <div class="float-right">
              <a class="copy-button" data-clipboard-text="User Name" href="#" onclick="return false;">Copy</a>
            </div>
          </div>
        </div>
        <div class="border-bottom mb10">
          <label><strong class="grey-color">Country:</strong></label>
          <div class="mb10 clearfix">
            <div class="float-left">
              <%= order.shipping_address['country'] %>
            </div>
            <div class="float-right">
              <a class="copy-button" data-clipboard-text="US" href="#" onclick="return false;">Copy</a>
            </div>
          </div>
        </div>
        <div class="border-bottom mb10">
          <label><strong class="grey-color">Address:</strong></label>
          <div class="mb10 clearfix">
            <div class="float-left">
              <%= order.shipping_address['address1'] %>
            </div>
            <div class="float-right">
              <a class="copy-button" data-clipboard-text="123 Fake Street" href="#" onclick="return false;">Copy</a>
            </div>
          </div>
        </div>
        <div class="border-bottom" style="display: none;">
          <label><strong class="grey-color">Address 2:</strong></label>
          <div class="mb10 clearfix">
            <div class="float-left">
              <%= order.shipping_address['address2'] %>
            </div>
            <div class="float-right">
              <a class="copy-button" href="#" onclick="return false;">Copy</a>
            </div>
          </div>
        </div>
      </div>
      <div class="margin-right flex-item-grid">
        <div class="border-bottom mb10">
          <label><strong class="grey-color">City:</strong></label>
          <div class="mb10 clearfix">
            <div class="float-left">
              <%= order.shipping_address['city'] %>
            </div>
            <div class="float-right">
              <a class="copy-button" data-clipboard-text="Dallas" href="#" onclick="return false;">Copy</a>
            </div>
          </div>
        </div>
        <div class="border-bottom mb10">
          <label><strong class="grey-color">Province:</strong></label>
          <div class="mb10 clearfix">
            <div class="float-left">
              Texas
            </div>
            <div class="float-right">
              <a class="copy-button" data-clipboard-text="Texas" href="#" onclick="return false;">Copy</a>
            </div>
          </div>
        </div>
        <div class="border-bottom mb10">
          <label><strong class="grey-color">Zip:</strong></label>
          <div class="mb10 clearfix">
            <div class="float-left">
              90268
            </div>
            <div class="float-right">
              <a class="copy-button" data-clipboard-text="90268" href="#" onclick="return false;">Copy</a>
            </div>
          </div>
        </div>
      </div>
      <div class="margin-right flex-item-grid">
        <div class="border-bottom mb10" style="display: none;">
          <label><strong class="grey-color">Phone:</strong></label>
          <div class="mb10 clearfix">
            <div class="float-left"></div>
            <div class="float-right">
              <a class="copy-button" href="#" onclick="return false;">Copy</a>
            </div>
          </div>
        </div>
        <div class="border-bottom mb10" style="display: none;">
          <label><strong class="grey-color">Note:</strong></label>
          <div class="mb10 clearfix">
            <div class="float-left"></div>
            <div class="float-right">
              <a class="copy-button" href="#" onclick="return false;">Copy</a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div>
    <div class="">
      <div class="panel-row" style="display: none;">
        <div class="container-flex flex-between border-bottom pb10">
          <div class="container-flex">
            <div class="mr10 ali-supplier"></div><strong class="display-inline grey-color margin-right">Order <span class="black-color">1 Ali Express items</span> from this order with a single click</strong>
          </div>
          <div class="container-flex align-right">
            <button class="btn btn-warning btn-icon-left"><span class="btn-icon-wrap"><svg class="icon-order">
            <use xlink:href="/img/app-icons.svg#icon-order" xmlns:xlink="http://www.w3.org/1999/xlink"></use></svg></span> Order Ali Express products</button>
          </div>
        </div>
      </div>
      <div class="panel-row" style="display: none;">
        <div class="container-flex flex-between border-bottom pb10">
          <div class="container-flex">
            <div class="mr10 ali-supplier"></div><strong class="display-inline grey-color margin-right">Get tracking code for <span class="black-color">1 Ali Express items</span> from this order with a single click</strong>
          </div>
          <div class="container-flex flex-right">
            <button class="btn btn-primary"><span class="oberlo-loader absolute-center loader-sm" style="display: none;"></span> <span class="">Get Tracking Code</span></button>
          </div>
        </div>
      </div>
    </div>
    <div class="panel-row overflow">
      <div class="">
        <div>
          <div class="container-flex flex-between">
            <div class="container-flex">
              <div class="container-flex">
                <span class="label label-warning to-order-status <% if order.ali_order_number %>hide<% end %>">To Order</span> <!---->
                <span class="label label-primary processing-status <% unless order.ali_order_number %>hide<% end %>">Processing</span> <!---->
                 <!---->
                 <!---->
                 <!---->
                 <!---->
              </div>
              <div class="svg-container ml5" style="display: none;">
                <div>
                  <a class="svg-container" href="#"><svg class="icon-envelope">
                  <use xlink:href="/img/app-icons.svg#icon-envelope" xmlns:xlink="http://www.w3.org/1999/xlink"></use></svg></a>
                </div>
              </div>
              <div class="container-flex">
                <span class="elephant-color ml30">Ali Order No:</span>
                <div class="container-flex margin-small-left">
                  <span style="display: none;" class="ali-order-form">
                    <input class="form-control input-sm ali-order-input" style="width: 150px;" type="text"> 
                    <a href="#" class="btn btn-default margin-small-left btn-sm ali-order-save">Save</a>
                  </span>

                  <% if order.ali_order_number.present? %>
                    <a class="blue-color ali-order-link" href="http://trade.aliexpress.com/order_detail.htm?orderId=<%= order.ali_order_number %>" target="_blank"><%= order.ali_order_number %></a>
                    <a class="btn btn-default btn-sm ali-order-add hide" href="#">Add</a>
                    <svg class="icon-info ml10 link-cursor ali-order-edit"><use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="/img/app-icons.svg#icon-info"></use></svg>
                  <% else %>
                    <a class="blue-color ali-order-link" href="javascript:;" target="_blank"></a>
                    <a class="btn btn-default btn-sm ali-order-add" href="#">Add</a>
                    <svg class="icon-info ml10 link-cursor ali-order-edit hide"><use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="/img/app-icons.svg#icon-info"></use></svg>
                  <% end %>


                </div>
              </div>
              <div class="margin-left" style="display: none;">
                <span class="elephant-color">Order No:</span> <span class="blue-color"></span>
              </div><!---->
              <div class="container-flex" style="display: none;">
                <svg class="icon-exclamation-circle ml10" data-original-title="" title="">
                <use xlink:href="/img/app-icons.svg#icon-exclamation-circle" xmlns:xlink="http://www.w3.org/1999/xlink"></use></svg> <span class="color-danger pl5">No Order Number</span> <a class="pl5" href="http://help.oberlo.com/orders/missing-orders/how-to-cancel-the-order-fulfillment-in-shopify" target="_blank">(What should I do?)</a>
              </div><svg class="icon-exclamation-circle ml10" data-original-title="" style="display: none;" title="">
              <use xlink:href="/img/app-icons.svg#icon-exclamation-circle" xmlns:xlink="http://www.w3.org/1999/xlink"></use></svg> <span class="elephant-color ml30">Shipped:&nbsp;</span> <strong><%= order.line_items.where(has_shipped: true).size %> / <%= order.line_items.size %></strong> <button class="btn-sm btn margin-small-left btn-default" type="button">Mark as Shipped</button> <!---->
            </div>
            <div class="container-flex align-right">
              <!---->
              <div class="align-right" style="">
                <script type="application/json" class="fulfillment-info-json">
                  <%= raw json_escape(order.fulfillment_info.to_json) %>
                </script>

                <span class="oberlo-loader dark v-align-middle loader-sm hide"></span>
                <button class="btn btn-warning btn-icon-left order-product <% if order.ali_order_number %>hide<% end %>">
                  <span class="btn-icon-wrap">
                    <svg class="icon-order"><use xlink:href="/img/app-icons.svg#icon-order" xmlns:xlink="http://www.w3.org/1999/xlink"></use></svg>
                  </span> Order Product
                </button>
                <button class="btn btn-primary get-tracking-code <% unless order.ali_order_number %>hide<% end %>">Get Tracking Code</button>
              </div>
              <div class="align-right" style="display: none;">
                <button class="btn btn-sm btn-default" type="button">Order Product</button>
              </div><button class="btn btn-primary align-center" style="display: none;"><span class="oberlo-loader absolute-center loader-sm" style="display: none;"></span> <span class="">Get Tracking Code</span></button> <button class="btn btn-default btn-sm" style="display: none;"><span class="oberlo-loader absolute-center loader-sm" style="display: none;"></span> <span class="">Get Tracking Code</span></button>
              <div style="display: none;">
                <div class="dropdown margin-left display-inline margin-right">
                  <a class="btn btn-default btn-sm btn-dropdown dropdown-toggle" data-toggle="dropdown" href="#"><span class="btn-icon-text">Action</span> <span class="btn-icon-wrap"><svg class="icon-open">
                  <use xlink:href="/img/app-icons.svg#icon-open" xmlns:xlink="http://www.w3.org/1999/xlink"></use></svg></span></a>
                  <div class="dropdown-menu dropdown-menu-right dropdown-sm">
                    <ul>
                      <!---->
                      <li>
                        <a href="#">Order Product</a>
                      </li>
                    </ul>
                  </div>
                </div>
              </div>
              <div style="display: none;">
                <a class="btn-link btn btn-icon btn-icon-right relative" href="#"><span class="btn-icon-text" style="display: none;">Show Products</span> <span class="btn-icon-text">Hide Products</span> <span class="btn-icon-wrap rotate180btn"><svg class="icon icon-arrow-more">
                <use xlink:href="./images/icons.svg#icon-small-arrow-down" xmlns:xlink="http://www.w3.org/1999/xlink"></use></svg></span></a>
              </div><!---->
              <!---->
            </div>
          </div>
        </div>
        <div>
          <!---->
          <div class="single-supplier-row margin-small-top">
            <table class="old-style">
              <!---->
              <tbody>
                <%= render order.line_items %>
              </tbody><!---->
              <!---->
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
